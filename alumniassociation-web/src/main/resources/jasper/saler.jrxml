		</parameter>
		<queryString>
			<![CDATA[select (@i := @i+1) AS rowno,t.*,substring_index(t.product_attr,'|', 1) as color,
case when t.pricing_method = '2' then t.buy_num else t.area end as countNum,
(select para_name from tb_sys_commpara where para_code = substring_index(color,':', 1) and para_key = substring_index(color,':', -1)) as pro_color,
(select para_name from tb_sys_commpara where para_code = 'unit' and para_key = t.unit) as unitStr,
case when t.pricing_method = '2' then (select para_name from tb_sys_commpara where para_code = 'unit' and para_key = t.unit) else 'm²' end as pricingMethod,
(select para_name from tb_sys_commpara where para_code = 'house_space' and para_key = t.space) as spaceStr
from (select @i:=0) as it,quotation_product t where t.order_id = $P{orderId}]]>
		</queryString>
